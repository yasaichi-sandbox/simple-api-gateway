{
  "name": "simple-api-gateway",
  "version": "0.0.1",
  "description": "",
  "author": "Yuichi Goto",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "codegen:kiota": "kiota generate --co -l typescript -d ./libs/fake-api-kiota/schema.yaml -o ./libs/fake-api-kiota/src/generated/ && ls && find ./libs/fake-api-kiota/src/generated/ -type f -exec sed -i '' -E \"s/from '\\.(.+)\\/';/from '.\\1\\/index.ts';/g\" {} +",
    "format": "deno fmt",
    "start": "deno run --allow-env --allow-net --allow-read=. src/main.ts",
    "start:dev": "deno run --allow-env --allow-net --allow-read=. --watch src/main.ts",
    "start:debug": "deno run --allow-env --allow-net --allow-read=. --inspect=127.0.0.1:9229 --watch src/main.ts",
    "start:prod": "deno run --allow-env --allow-net --allow-read=. src/main.ts",
    "lint": "deno lint",
    "test": "deno test --allow-env --allow-read=. --ignore=test/ --parallel",
    "test:watch": "deno task test --watch",
    "test:cov": "deno task test --coverage && deno coverage --exclude='/generated/' && deno coverage --exclude='/generated/' --lcov --output=coverage/lcov.info",
    "test:debug": "deno test --allow-env --allow-read=. --inspect-brk=127.0.0.1:9230",
    "test:e2e": "deno test --allow-env --allow-net --allow-read=. --parallel test/"
  },
  "dependencies": {
    "@microsoft/kiota-abstractions": "1.0.0-preview.48",
    "@microsoft/kiota-http-fetchlibrary": "1.0.0-preview.47",
    "@microsoft/kiota-serialization-form": "1.0.0-preview.37",
    "@microsoft/kiota-serialization-json": "1.0.0-preview.48",
    "@microsoft/kiota-serialization-multipart": "1.0.0-preview.26",
    "@microsoft/kiota-serialization-text": "1.0.0-preview.44",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "effect": "^2.4.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "type-fest": "^4.13.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@std/testing": "npm:@jsr/std__testing@^0.220.0",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.4",
    "@types/node": "^20.3.1",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.0",
    "fetch-mock": "^9.11.0",
    "sinon": "^17.0.1",
    "supertest": "^6.3.3"
  }
}
